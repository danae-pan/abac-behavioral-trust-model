{
    "policies": [
        {
            "uid": "policy-1-regional-admin-edit-student-enrollment-county",
            "description": "Admin from the same county can maintain (view & edit) student enrollment with trust >= 0.5",
            "effect": "allow",
            "rules": {
                "subject": {
                    "$.role": {
                        "condition": "Equals",
                        "value": "Admin"
                    },
                    "$.trust_value": {
                        "condition": "Gte",
                        "value": 0.5
                    }
                },
                "resource": {
                    "$.obj_type": {
                        "condition": "Equals",
                        "value": "student enrollment"
                    },
                    "$.county_code": {
                        "condition": "IsInAttribute",
                        "ace": "subject",
                        "path": "$.county_code"
                    }
                },
                "action": {
                    "$.method": {
                        "condition": "IsIn",
                        "values": [
                            "view",
                            "edit"
                        ]
                    }
                },
                "context": {}
            },
            "targets": {},
            "priority": 0
        },

